<html lang="en">
<head>
    <title>Flickr gallery</title>
    <link rel="stylesheet" type="text/css" href="http://yui.yahooapis.com/pure/0.4.2/pure-min.css">
    <link rel="stylesheet" type="text/css" href="/css/styles.css">
</head>
<body class="pure">

<h1>Flickr gallery</h1>

<div class="photos">
    {{#each photos}}
        <div class="photo">
            <img src='{{this.url_n}}' width="{{this.width_n}}" height="{{height_n}}" title="{{this.title}}">
        </div>
    {{/each}}

</div>


</body>
</html>
